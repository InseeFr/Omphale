<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="fr.insee.omphale.domaine.projection">
	<class name="TypeParam" table="TYPE_PARAM">
		<id name="code" column="TYPE_PARAM" type="string" length="10">
		</id>
		<property name="valDef" column="VAL_DEF" type="double"></property>
		<property name="libelle" column="LIBELLE" type="string"
			length="50"></property>
		<property name="entier" column="IS_ENTIER" type="boolean"></property>
	</class>
</hibernate-mapping>

